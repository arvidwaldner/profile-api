{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ApiKeys": {
    "localhost4200": "your-service-api-key-here",
    "prodtest:": "your-service_api-key-here"
  },
  "AudienceApiKeyMap": [
    {
      "Audience": "https://localhost:4200",
      "ApiKeyName": "localhost4200"
    },
    {
      "Audience": "prodtest",
      "ApiKeyName": "prodtest"
    }
  ],
  "Jwt": {
    "Issuer": "your-issuer-here",
    "SigningKey": "your-signing-key-here",
    "Audiences": [ "https://localhost:4200", "prodtest" ]
  }
}
